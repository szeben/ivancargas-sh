<odoo>
  <data>
    <record model="ir.ui.view" id="stock_picking_view_form_inherit">
      <field name="name">stock.picking.view.form.inherit</field>
      <field name="model">stock.picking</field>
      <field name="inherit_id" ref="studio_customization.odoo_studio_stock_pi_aa0e8409-ca89-4a71-886b-2739ae64914c"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='x_studio_wbgua']" position="attributes">
          <attribute name="attrs">{"invisible": [["x_studio_operacion_transporte","=",False]], "required": [["x_studio_operacion_transporte","=",True]]}</attribute>
        </xpath>

        <xpath expr="//field[@name='sale_id']" position="after">
          <field name="product_id_related" invisible="1"/>
          <field name="purchase_order" widget="selection" attrs="{'required': [('x_studio_operacion_transporte', '=', True)], 'invisible': [('x_studio_operacion_transporte','=',False)]}" domain="[('id', 'in', product_id_related)]" />
        </xpath>
      </field>
    </record>

    <record model="ir.ui.view" id="purchase_order_view_form_inherit">
      <field name="name">purchase.order.view.form.inherit</field>
      <field name="model">purchase.order</field>
      <field name="inherit_id" ref="purchase.purchase_order_form"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='currency_id']" position="after">
          <field name="wbgua" readonly="1"/>
        </xpath>
      </field>
    </record>

    <record model="ir.ui.view" id="purchase_order_view_list_inherit">
      <field name="name">purchase.order.view.list.inherit</field>
      <field name="model">purchase.order</field>
      <field name="inherit_id" ref="purchase.purchase_order_view_tree"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='user_id']" position="after">
          <field name="wbgua" readonly="1"/>
        </xpath>
      </field>
    </record>

    <record model="ir.ui.view" id="transport_entry_view_form_inherit_odoo_studio">
      <field name="name">transport.entry.view.form.inherit.odoo.studio</field>
      <field name="model">transport.entry</field>
      <field name="inherit_id" ref="studio_customization.odoo_studio_transpor_13d599dc-1305-49d1-b107-b71814313789"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='sale_order']" position="replace">
          <field name="sale_order_id" string="Orden de Venta" />
          <field name="purchase_order_id"/>
        </xpath>
      </field>
    </record>

    <record model="ir.ui.view" id="transport_entry_view_list_inherit">
      <field name="name">transport.entry.view.list.inherit</field>
      <field name="model">transport.entry</field>
      <field name="inherit_id" ref="transport_module.view_transport_entry_tree"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='date']" position="after">
          <field name="purchase_order_id"/>
        </xpath>
      </field>
    </record>

    <record model="ir.ui.view" id="sale_order_view_list_inherit">
      <field name="name">sale.order.view.list.inherit</field>
      <field name="model">sale.order</field>
      <field name="inherit_id" ref="routes_in_invoices.view_order_form_inherit"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='order_route']" position="after">
          <field name="wbgua" readonly="1"/>
        </xpath>
      </field>
    </record>

    <record model="ir.ui.view" id="sale_order_view_tree_inherit">
      <field name="name">sale.order.view.tree.inherit</field>
      <field name="model">sale.order</field>
      <field name="inherit_id" ref="sale.view_order_tree"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='partner_id']" position="after">
          <field name="wbgua" readonly="1" />
        </xpath>
      </field>
    </record>

    <record model="ir.ui.view" id="account_move_view_list_inherit">
      <field name="name">account.move.view.list.inherit</field>
      <field name="model">account.move</field>
      <field name="inherit_id" ref="account.view_move_form"/>
      <field name="arch" type="xml">
        <xpath expr="//tree/field[@name='quantity']" position="before">
          <field name="wbgua_related" readonly="1" attrs="{'column_invisible': [('parent.move_type','!=','out_invoice')]}"/>
        </xpath>
      </field>
    </record>

    <record model="ir.ui.view" id="account_move_view_form_inherit">
      <field name="name">account.move.view.form.inherit</field>
      <field name="model">account.move</field>
      <field name="inherit_id" ref="account.view_move_form"/>
      <field name="arch" type="xml">
        <xpath expr="//field[@name='invoice_control_number']" position="after">
          <field name="wbgua_account_move" readonly="1" attrs="{ 'invisible': [('move_type','!=','in_invoice')]}"/>
        </xpath>
      </field>
    </record>

  </data>
</odoo>